config setup
    charondebug="all"
    uniqueids=yes
    strictcrlpolicy=no

conn L2TP-PSK
    type=transport
    authby=secret
    left=%defaultroute
    leftprotoport=17/1701
    right=VPN_SERVER_IP_PLACEHOLDER
    rightid=RIGHTID_PLACEHOLDER
    rightprotoport=17/%any
    auto=start
    keyexchange=ikev1
    ike=aes128-sha1-modp2048,aes128-sha1-modp1024,3des-sha1-modp1024!
    esp=aes128-sha1,3des-sha1!
    keyingtries=%forever
